body{
    background: #fff1ba!important;
    font-family: "微软雅黑"!important;

}
.fontStyle{
    color: #412600;
    font-size: 17px;
    padding-left: 5%;
    font-family: "微软雅黑"!important;
}
.fontTitle{/* 公共的类 */
    color: #302a07;
    font-size: 2em;
    font-weight: bold;
    font-family: "微软雅黑"!important;
}
.fontTitle span{
    color: #a0903a;
    font-size: 1em;

}
.fcred{
    color:rgb(242, 35, 35);
    font-size: 0.5em;
    display: block;
    margin-left: 85%;
    margin-top: -5%;
}
.IDInfo{
    color: #ff9938;
    font-size: 16px;
    font-weight: bold;
}
.InfoName{
    color: #a69644;
    font-size: 22px;
}

.main{
    margin: 0 auto;
    position: relative;
}
.bdBg{
    background: #fff1ba url(../img/banner.jpg) center top no-repeat;
    position: relative;
    width: 100%;
    height: 865px;
}


.postBar{
    display: block;
    width: 213px;
    height: 59px;
    background: url(../img/tieba.png) 0 0 no-repeat;
    position: absolute;
    left: 47%;
    top: 64%;
}
.tribune{
    display: block;
    width: 213px;
    height: 59px;
    background:  url(../img/tribune.png) 0 0 no-repeat;
    position: absolute;
    left: 60%;
    top: 64%;
}
.switchBtn{
    width: 198px;
    height: 70px;
    position: absolute;
    top: 85%;
    overflow: hidden;
}
.indexBtn{
    left:36%;
}
.worksBtn{
    left: 49%;
}
.switchBtn img{
    position: absolute;
    left: 0;
}
.indexBtn img{
    top:0;
}
.worksBtn img{
    top: -70px;
}
.btnActiveC img{
    left:-198px;
}
.btnActive img{
    left:-198px;
}

/* 内容部分 */
.conBox{
    margin:0 auto;
}
.indexBox{
    margin-left: -6%;
}
.hideBox{
    display: none;
}
.timebox{
    margin-left: 6%;
}
.timebox2{
    margin-left: 4%;
}
.timeImg1{
    margin-left: 21%;
}
.timeImg2{
    margin-left: 6%;
}
.timeImg3{
    margin-left: -8%;
}
.prize{
    text-align: center;
    margin-right: 6%;
}
.footIcon{
    width: 100%;
    text-align: center;
    position: relative;
}
.goodworks{
    margin: 0 auto;
}
.ulList{
    padding-left: 8%;
}
.listLi{
    width: 100%;
    border-radius: 10px;
    background: #f2e5ae;
    overflow: hidden;
    padding: 10px;
    margin-bottom: 5%;
}
.logoImg{
    border-radius:32px;
    float: left;
    margin-left: 10px;

}

.infoBox{
    float: left;
    margin-left: 6%;
}
.detailBtn{
    float: left;
    margin-left: 20%;
}
.ulList{
    margin-left: -6%;
}
.prizeBox{
    margin-left: 12%;
}
.rater{
    margin-right: 3%;
}
.raterBox{
    margin-left: 13%;
}

